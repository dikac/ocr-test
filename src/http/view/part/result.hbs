<div>
{{#if data}}

    <table>
        <thead>
        <tr>
            <td>{{data.headerProvince}}</td>
            <td>{{data.headerCity}}</td>
        </tr>
        </thead>
        <tbody>

        <tr>
            <td>Nik</td>
            <div>{{data.nik}}</div>
        </tr>

        <tr>
            <td>Nama</td>
            <div>{{data.name}}</div>
        </tr>

        <tr>
            <td>Tempat/Tgl Lahir</td>
            <td>{{data.placeAndDateBirth}}</td>
        </tr>

        <tr>
            <td>Jenis Kelamin</td>
            <td>{{data.gender}}</td>
        </tr>

        <tr>
            <td>Alamat</td>
            <td>{{data.alamat}}</td>
            <td>{{data.rtRw}}</td>

            <td>{{data.kelurahanDesa}}</td>
            <td>{{data.kecamatan}}</td>
        </tr>

        <tr>
            <td>Agama</td>
            <td>{{data.religion}}</td>
        </tr>

        <tr>
            <td>Status Perkawinan</td>
            <td>{{data.marital}}</td>
        </tr>

        <tr>
            <td>Pekerjaan</td>
            <td>{{data.job}}</td>
        </tr>

        <tr>
            <td>Kewarganegaraan</td>
            <td>{{data.nationality}}</td>
        </tr>

        <tr>
            <td>Berlaku</td>
            <td>{{data.validUntil}}</td>
        </tr>

        <tr>
            <td>Golongan Darah</td>
            <td>{{data.bloodType}}</td>
        </tr>

        </tbody>
    </table>

{{/if}}

</div>

